@model IEnumerable<Entity.Scheduale>
<div class="panel">
    <div class="panel-heading">
        Employee Schedualed Visits
    </div>
    <div class="panel-body">
        <table id="datatable" class="table  table-hover table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Month</th>
                    <th>Employee</th>
                    <th>Customer</th>
                    <th>Visit Date</th>
                </tr>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Month</td>
                    <td>@item.ManEmp.Name</td>
                    <td>@item.Customers.Name</td>
                </tr>
                }
            </tbody>
        </table>
        <div class="alert hide" id="errorHandler"></div>
    </div>
</div>
