<div class="panel">
    <div class="panel-heading">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                Review To Approve Plans
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <div class="pull-right">
                    <button class="btn btn-success" disabled onclick="Approve()" id="approvebtn"> Approve  Month Plan</button>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-4 col-sm-6 col-xs-12 form-group">
                <label>Employee</label>
                @Html.Action("Select","Employees")
            </div>
            <div class="col-md-4 col-sm-6 col-xs-12 form-group">
                <label>Month</label>
                <select class="form-control input-sm" id="month">
                    <option value="1" selected>Jan</option>
                    <option value="2">Feb</option>
                    <option value="3">March</option>
                    <option value="4">April</option>
                    <option value="5">May</option>
                    <option value="6">Jun</option>
                    <option value="7">Jul</option>
                    <option value="8">Aug</option>
                    <option value="9">Sep</option>
                    <option value="10">Oct</option>
                    <option value="11">Nov</option>
                    <option value="12">Dec</option>
                </select>
            </div>
        </div>
        <button class="btn btn-primary" onclick="Review()"> Get Plan </button>
        <hr />
        <div id="View"></div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/Requests/PlanRequests.js"></script>

